{
    "name": "emulienfou/mongodb-pack",
    "type": "symfony-pack",
    "license": "MIT",
    "description": "A pack for the Doctrine MongoDB ODM",
    "require": {
        "php": "^7.2",
        "doctrine/mongodb-odm": "^2.0",
        "doctrine/mongodb-odm-bundle": "^4.0"
    },
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        }
    },
    "provide": {
        "doctrine/implementation": "1.0",
        "doctrine/mongodb-implementation": "1.0"
    }
}
