{
    "name": "emulienfou/mongodb-pack",
    "type": "symfony-pack",
    "license": "MIT",
    "description": "A pack for the Doctrine MongoDB ODM",
    "repositories": [{"type": "vcs","url": "https://github.com/emulienfou/DoctrineMongoDBBundle"}],
    "require": {
        "php": "^7.0",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/mongodb-odm": "1.3.x-dev",
        "doctrine/mongodb-odm-bundle": "3.6.x-dev",
        "doesntmattr/mongodb-migrations-bundle": "^2.0"
    },
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        }
    },
    "provide": {
        "doctrine/implementation": "1.0",
        "doctrine/mongodb-implementation": "1.0"
    }
}
